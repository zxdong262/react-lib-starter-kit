{
  "name": "react-lib-starter-kit",
  "version": "0.0.1",
  "description": "starter kit for creating react lib with webpack/babel/es6/es7, unit test/ci/codecoverage/umd/source map included",
  "main": "dist/react-lib-starter-kit.js",
  "scripts": {
    "build": "rm dist/*.* && NODE_ENV=production ./node_modules/.bin/webpack -p --progress",
    "start": "./node_modules/.bin/npm-run-all -p dev:server dev:client ",
    "dev:server": "node build/dev-server",
    "dev:server-debug": "node --debug build/dev-server",
    "dev:client": "./node_modules/.bin/webpack-dev-server --progress --colors --inline --content-base dist/",
    "test": "./node_modules/.bin/karma start ./test/karma.conf.js"
  },
  "author": {
    "email": "zxdong@gmail.com",
    "name": "ZHAO Xudong",
    "url": "http://html5beta.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zxdong262/react-lib-starter-kit"
  },
  "keywords": [
    "react",
    "starter-kit"
  ],
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-factory": "^6.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "bootstrap": "^4.0.0-alpha.4",
    "jquery": "^3.1.0",
    "karma": "*",
    "karma-chai-plugins": "*",
    "karma-coverage": "*",
    "karma-mocha": "*",
    "karma-phantomjs-launcher": "*",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "koa": "^1.2.4",
    "koa-mount": "^1.3.0",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mocha": "*",
    "npm-run-all": "^3.1.0",
    "open-browser-webpack-plugin": "0.0.2",
    "phantomjs-prebuilt": "*",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "run-sequence": "^1.2.1",
    "unminified-webpack-plugin": "^1.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "bugs": {
    "url": "https://github.com/zxdong262/react-lib-starter-kit/issues"
  },
  "homepage": "https://github.com/zxdong262/react-lib-starter-kit"
}
