{
  "name": "react-lib-starter-kit",
  "version": "0.1.0",
  "description": "starter kit for creating react lib with webpack/babel/es6/es7, unit test/ci/codecoverage/umd/source map included",
  "main": "dist/react-lib-starter-kit.js",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "start": "./node_modules/.bin/run-p dev:server dev:client build",
    "dev:server": "node build/dev-server",
    "dev:server-debug": "node --debug build/dev-server",
    "dev:client": "./node_modules/.bin/webpack-dev-server --colors --inline --content-base dist/",
    "test": "./node_modules/.bin/karma start ./test/karma.conf.js"
  },
  "author": {
    "email": "zxdong@gmail.com",
    "name": "ZHAO Xudong",
    "url": "http://html5beta.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zxdong262/react-lib-starter-kit"
  },
  "keywords": [
    "react",
    "starter-kit"
  ],
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-es2015-modules-factory": "6.0.0",
    "babel-plugin-transform-es2015-modules-umd": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2015-loose": "8.0.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-preset-stage-1": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "bootstrap": "3.3.7",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "jquery": "3.1.1",
    "karma": "1.4.1",
    "karma-chai-plugins": "0.8.0",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "koa": "^1.2.4",
    "koa-mount": "^1.3.0",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.1",
    "open-browser-webpack-plugin": "0.0.3",
    "phantomjs-prebuilt": "2.1.14",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.4.2",
    "unminified-webpack-plugin": "1.2.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.2.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "bugs": {
    "url": "https://github.com/zxdong262/react-lib-starter-kit/issues"
  },
  "homepage": "https://github.com/zxdong262/react-lib-starter-kit"
}
